{
  "name": "zmpl",
  "displayName": "Zmpl",
  "description": "Zmpl language grammar. Zmpl is a templating language for Zig.",
  "version": "1.1.1",
  "publisher": "uzyn",
  "author": {
    "name": "U-Zyn Chua",
    "email": "chua@uzyn.com",
    "url": "https://uzyn.com/"
  },
  "license": "MIT",
  "repository": {
    "url": "https://github.com/uzyn/zmpl-grammar",
    "type": "git"
  },
  "homepage": "https://github.com/uzyn/zmpl-grammar",
  "engines": {
    "vscode": "^1.98.0"
  },
  "icon": "icon.png",
  "categories": ["Programming Languages"],
  "contributes": {
    "languages": [{
      "id": "zmpl",
      "aliases": ["Zmpl", "zmpl"],
      "extensions": [".zmpl"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "zmpl",
      "scopeName": "text.html.zmpl",
      "path": "./syntaxes/zmpl.tmLanguage.json",
      "embeddedLanguages": {
        "text.html.basic": "html"
      }
    }]
  }
}
